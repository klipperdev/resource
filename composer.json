        "symfony/dependency-injection": "^5.1.0",
        "symfony/event-dispatcher": "^5.1.0",
        "symfony/validator": "^5.1.0",
        "symfony/http-foundation": "^5.1.0",
        "symfony/expression-language": "^5.1.0",
        "symfony/translation": "^5.1.0",
        "doctrine/persistence": "^1.0.0",
        "doctrine/orm": "^2.6.0",
        "klipper/doctrine-extra": "^2.0.0",
        "klipper/doctrine-extensions": "^2.0.0",
        "klipper/model-contracts": "^2.0.0"
    },
    "require-dev": {
