{
    "name": "klipper/resource",
    "type": "library",
    "description": "Resource component of Klipper",
    "keywords": ["resource", "doctrine", "batch", "manager", "validation"],
    "homepage": "https://github.com/klipperdev/resource",
    "license": "MIT",
    "authors": [
        {
            "name": "Fran√ßois Pluchino",
            "email": "francois.pluchino@klipper.dev"
        }
    ],
